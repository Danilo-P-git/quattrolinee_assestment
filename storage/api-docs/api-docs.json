                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Concerto Jazz"
                    },
                    "description": {
                        "type": "string",
                        "example": "Concerto jazz dal vivo"
                    },
                    "length": {
                        "type": "number",
                        "format": "float",
                        "example": 3
                    },
                    "start_date": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-07-15 21:00:00"
                    },
                    "category_id": {
                        "type": "integer",
                        "example": 2
                    }
                },
                "type": "object"
            },
            "Event": {
                "title": "Evento",
                "description": "Schema di un evento",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Concerto Jazz"
                    },
                    "description": {
                        "type": "string",
                        "example": "Concerto jazz dal vivo"
                    },
                    "length": {
                        "type": "number",
                        "format": "float",
                        "example": 3
                    },
                    "start_date": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-07-15 21:00:00"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
